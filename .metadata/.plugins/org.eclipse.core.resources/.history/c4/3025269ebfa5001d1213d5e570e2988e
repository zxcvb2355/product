 <mapper namespace="main.a.b.product.common.chabun.dao.ProductChabunDAO">
 
 <select id="getProductChabun" resultType="String">
 
 	SELECT ISNULL(MAX(RIGHT(PNUM, 4)), 0) + 1 AS PNUM
 	FROM CPST_PRODUCT A 
 	
 
 </select>
 
  
 </mapper>